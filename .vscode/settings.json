{
    "window.zoomLevel": 0,
    "files.autoSave": "onFocusChange",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "eslint.autoFixOnSave": true,
    "eslint.enable": true,
    "eslint.options": { "configFile": ".eslintrc.json" },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      { "language": "html", "autoFix": true }
    ],
    "prettier.singleQuote": true,
    "prettier.semi": false,
    "prettier.tabWidth": 2,
  }
  